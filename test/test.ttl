@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

cc:Document a owl:Class
;  rdfs:label "Document"@en
;  rdfs:label "Documento"@es
;  rdfs:subClassOf skos:Concept
.

cc:Article a owl:Class
;  rdfs:label "Article"@en
;  rdfs:subClassOf skos:Concept
.

cc:published a owl:DatatypeProperty
;  rdfs:domain  skos:Concept
;  rdfs:label "published"@en
;  rdfs:range xsd:dateTime
.

cc:test a cc:Document
;  skos:prefLabel "Test page H"@en
;  cc:published "2018-10-08T23:08:00.000+01:00"^^xsd:dateTime
.

cc:testmarkdown a cc:Document, cc:Article
;  skos:prefLabel "Test page M"@en
.

<testinlinemarkdown> a cc:Document;
;  skos:prefLabel "Test page I"@en
;  skos:related cc:test
;  skos:related cc:testmarkdown
;  cc:markdown """
