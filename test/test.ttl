@prefix cc: <http://www.colourcountry.net/thing/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

cc:Document a owl:Class
;  rdfs:label "Document"@en
;  rdfs:subClassOf skos:Concept
.

cc:title a owl:DatatypeProperty
;  rdfs:domain  skos:Concept
;  rdfs:label "title"@en
;  rdfs:range xsd:string
.

cc:published a owl:DatatypeProperty
;  rdfs:domain  skos:Concept
;  rdfs:label "published"@en
;  rdfs:range xsd:dateTime
.

<test> a cc:Document
;  cc:title "Test page H"@en
;  cc:published "2018-10-08T23:08:00.000+01:00"^^xsd:dateTime
.

<testmarkdown> a cc:Document
;   cc:title "Test page M"@en
.

