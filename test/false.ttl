@prefix f: <http://www.colourcountry.net/false/model/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

f:CreativeWork a owl:Class
;  rdfs:label "Article"@en
;  rdfs:subClassOf skos:Concept
.

f:Document a owl:Class
;  rdfs:label "Document"@en
;  rdfs:subClassOf f:CreativeWork
.

f:Picture a owl:Class
;  rdfs:label "Picture"@en
;  rdfs:subClassOf f:Document
.

f:Media a owl:Class
;  rdfs:label "Media"@en
;  rdfs:subClassOf skos:Concept
.

f:Context a owl:Class
;  rdfs:label "Context"@en
;  rdfs:subClassOf skos:Concept
.

f:targetContext a owl:ObjectProperty
;  rdfs:label "target context"@en
;  rdfs:domain f:Media
;  rdfs:range f:Context
;  rdfs:subClassOf skos:related
;  owl:inverseOf f:targetContextOf
.

f:targetContextOf a owl:ObjectProperty
;  rdfs:label "target context of"@en
;  rdfs:domain f:Media
;  rdfs:range f:Context
;  rdfs:subClassOf skos:related
;  owl:inverseOf f:targetContext
.

f:pageContext a f:Context
;  rdfs:label "Content on its own page"
.

f:embedContext a f:Context
;  rdfs:label "Embedded content within a document page"
.

f:teaserContext a f:Context
;  rdfs:label "Preview of content for teasers and promotion"
.

f:mediaType a owl:DatatypeProperty
;  rdfs:label "media type"@en
;  rdfs:domain f:Media
;  rdfs:range xsd:string
.

f:charset a owl:DatatypeProperty
;  rdfs:label "character set"@en
;  rdfs:domain f:Media
;  rdfs:range xsd:string
.

f:rendition a owl:DatatypeProperty
;  rdfs:label "Rendition"@en
;  rdfs:domain f:CreativeWork
;  rdfs:range f:Media
.

f:Site a owl:Class
;  rdfs:label "Web site"@en
;  rdfs:subClassOf f:CreativeWork
.

f:description a owl:DatatypeProperty
;  rdfs:label "description"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

f:published a owl:DatatypeProperty
;  rdfs:label "published"@en
;  rdfs:domain f:Document
;  rdfs:range xsd:dateTime
.

f:updated a owl:DatatypeProperty
;  rdfs:label "updated"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:dateTime
.

f:featuresOn a owl:ObjectProperty
;  rdfs:label "features on"@en
;  rdfs:domain f:CreativeWork
;  rdfs:range f:Site
;  rdfs:subClassOf skos:broader
;  owl:inverseOf f:features
.

f:features a owl:ObjectProperty
;  rdfs:label "features"@en
;  rdfs:domain f:Site
;  rdfs:range f:CreativeWork
;  rdfs:subClassOf skos:narrower
;  owl:inverseOf f:featuredOn
.

f:about a owl:ObjectProperty
;  rdfs:label "about"@en
;  rdfs:domain f:CreativeWork
;  rdfs:range skos:Concept
;  rdfs:subClassOf skos:related
.

f:pictureOf a owl:ObjectProperty
;  rdfs:label "picture of"@en
;  rdfs:domain f:Picture
;  rdfs:range skos:Concept
;  rdfs:subClassOf f:about
.
