@prefix : <http://www.colourcountry.net/false/model/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

:CreativeWork a owl:Class
;  rdfs:label "CreativeWork"@en
;  rdfs:subClassOf skos:Concept
.

:Content a owl:Class
;  rdfs:label "Content"@en
;  rdfs:subClassOf :CreativeWork
.

:Document a owl:Class
;  rdfs:label "Document"@en
;  rdfs:subClassOf :Content
.

:Picture a owl:Class
;  rdfs:label "Picture"@en
;  rdfs:subClassOf :Content
.

:Media a owl:Class
;  rdfs:label "Media"@en
;  rdfs:subClassOf skos:Concept
.

:Context a owl:Class
;  rdfs:label "Context"@en
;  rdfs:subClassOf skos:Concept
.

:targetContext a owl:ObjectProperty
;  rdfs:label "target context"@en
;  rdfs:domain :Media
;  rdfs:range :Context
;  rdfs:subClassOf skos:related
;  owl:inverseOf :targetContextOf
.

:targetContextOf a owl:ObjectProperty
;  rdfs:label "target context of"@en
;  rdfs:domain :Media
;  rdfs:range :Context
;  rdfs:subClassOf skos:related
;  owl:inverseOf :targetContext
.

:pageContext a :Context
;  rdfs:label "Content on its own page"
.

:embedContext a :Context
;  rdfs:label "Embedded content within a document page"
.

:teaserContext a :Context
;  rdfs:label "Preview of content for teasers and promotion"
.

:mediaType a owl:DatatypeProperty
;  rdfs:label "media type"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:charset a owl:DatatypeProperty
;  rdfs:label "character set"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:blobURL a owl:DatatypeProperty
;  rdfs:label "blob URL"@en
;  rdfs:domain :Media
;  rdfs:range xsd:anyURI
.

:rendition a owl:DatatypeProperty
;  rdfs:label "Rendition"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :Media
.

:Site a owl:Class
;  rdfs:label "Web site"@en
;  rdfs:subClassOf :Content
.

:description a owl:DatatypeProperty
;  rdfs:label "description"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:markdown a owl:DatatypeProperty
;  rdfs:label "markdown"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:html a owl:DatatypeProperty
;  rdfs:label "HTML"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:url a owl:DatatypeProperty
;  rdfs:label "Web URL"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:anyURI
.

:published a owl:DatatypeProperty
;  rdfs:label "published"@en
;  rdfs:domain :Document
;  rdfs:range xsd:dateTime
.

:updated a owl:DatatypeProperty
;  rdfs:label "updated"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:dateTime
.

:featuresOn a owl:ObjectProperty
;  rdfs:label "features on"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :Site
;  rdfs:subClassOf skos:broader
;  owl:inverseOf :features
.

:features a owl:ObjectProperty
;  rdfs:label "features"@en
;  rdfs:domain :Site
;  rdfs:range :CreativeWork
;  rdfs:subClassOf skos:narrower
;  owl:inverseOf :featuresOn
.

:usedIn a owl:ObjectProperty
;  rdfs:label "used in"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :CreativeWork
;  rdfs:subClassOf skos:broader
;  owl:inverseOf :uses
.

:uses a owl:ObjectProperty
;  rdfs:label "uses"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :CreativeWork
;  rdfs:subClassOf skos:narrower
;  owl:inverseOf :usedIn
.

:about a owl:ObjectProperty
;  rdfs:label "about"@en
;  rdfs:domain :CreativeWork
;  rdfs:range skos:Concept
;  rdfs:subClassOf skos:related
.

:pictureOf a owl:ObjectProperty
;  rdfs:label "picture of"@en
;  rdfs:domain :Picture
;  rdfs:range skos:Concept
;  rdfs:subClassOf :about
.
