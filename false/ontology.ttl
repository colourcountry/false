@prefix : <http://www.colourcountry.net/false/model/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

:CreativeWork a owl:Class
;  rdfs:label "creative work"@en
;  rdfs:subClassOf skos:Concept
.

:Project a owl:Class
;  rdfs:label "project"@en
;  rdfs:subClassOf :CreativeWork
;  skos:scopeNote "A creative activity with an overall focus or intent"@en
.

:Content a owl:Class
;  rdfs:label "content"@en
;  rdfs:subClassOf :CreativeWork
;  skos:scopeNote "A work which can be rendered in two-dimensional visual form, especially on or as a Web site"@en
.

:Site a owl:Class
;  rdfs:label "Web site"@en
;  rdfs:subClassOf :Content
.

:Page a owl:Class
;  rdfs:label "Web page"@en
;  rdfs:subClassOf :Content
.

:Document a owl:Class
;  rdfs:label "document"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be read. Normally text based but may also be made of images (cartoon, graphic novel...)"@en
.

:Picture a owl:Class
;  rdfs:label "picture"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be viewed, whose meaning does not rely on the way it changes through time."@en
.

:Video a owl:Class
;  rdfs:label "video"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be watched."@en
.

:Media a owl:Class
;  rdfs:label "media"@en
.

:Context a owl:Class
;  rdfs:label "context"@en
;  rdfs:subClassOf skos:Concept
.

:asPage a :Context
;  rdfs:label "page context"@en
.

:asEmbed a :Context
;  rdfs:label "embedded context"@en
.

:asLink a :Context
;  rdfs:label "link context"@en
.

:mediaType a owl:DatatypeProperty
;  rdfs:label "media type"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:charset a owl:DatatypeProperty
;  rdfs:label "character set"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:blobURL a owl:DatatypeProperty
;  rdfs:label "blob URL"@en
;  rdfs:domain :Media
;  rdfs:range xsd:anyURI
.

:rendition a owl:DatatypeProperty
;  rdfs:label "rendition"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :Media
.

:renditionOf a owl:DatatypeProperty
;  rdfs:label "rendition of"@en
;  owl:inverseOf :rendition
.

:description a owl:DatatypeProperty
;  rdfs:label "description"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:renditionOf a owl:DatatypeProperty
;  rdfs:label "description of"@en
;  owl:inverseOf :description
.

:markdown a owl:DatatypeProperty
;  rdfs:label "markdown"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:html a owl:DatatypeProperty
;  rdfs:label "HTML"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:url a owl:DatatypeProperty
;  rdfs:label "Web URL"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:anyURI
.

:embedPath a owl:DatatypeProperty
;  rdfs:label "FALSE internal embed path"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:anyURI
.


:published a owl:DatatypeProperty
;  rdfs:label "published"@en
;  rdfs:domain :Document
;  rdfs:range xsd:dateTime
.

:updated a owl:DatatypeProperty
;  rdfs:label "updated"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:dateTime
.


:features a owl:ObjectProperty
;  rdfs:label "features"@en
;  rdfs:domain :Site
;  rdfs:range :CreativeWork
;  rdfs:subClassOf skos:narrower
.

:featuresOn a owl:ObjectProperty
;  rdfs:label "features on"@en
;  rdfs:subClassOf skos:broader
;  owl:inverseOf :features
.

:includes a owl:ObjectProperty
;  rdfs:label "includes"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :CreativeWork
;  rdfs:subClassOf skos:narrower
.

:includedIn a owl:ObjectProperty
;  rdfs:label "included in"@en
;  rdfs:subClassOf skos:broader
;  owl:inverseOf :includes
.

:about a owl:ObjectProperty
;  rdfs:label "about"@en
;  rdfs:domain :CreativeWork
;  rdfs:range skos:Concept
;  rdfs:subClassOf skos:related
.

:hasAbout a owl:ObjectProperty
;  rdfs:label ""@en # !
;  rdfs:subClassOf skos:related
;  owl:inverseOf :about
;  rdfs:subClassOf skos:related
.

:depicts a owl:ObjectProperty
;  rdfs:label "depicts"@en
;  rdfs:domain :Picture
;  rdfs:range skos:Concept
;  rdfs:subClassOf :about
.

:depictedIn a owl:ObjectProperty
;  rdfs:label "depicted in"@en
;  owl:inverseOf :depicts
;  rdfs:subClassOf :hasAbout
.

:subClass a rdf:Property
;  owl:inverseOf rdfs:subClassOf
.

:typeOf a rdf:Property
;  owl:inverseOf rdf:type
.

:domainOf a rdf:Property
;  owl:inverseOf rdfs:domain
.

:rangeOf a rdf:Property
;  owl:inverseOf rdfs:range
.

