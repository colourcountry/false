@prefix : <http://id.colourcountry.net/false/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

skos:prefLabel rdfs:subPropertyOf rdfs:label .

:InverseDatatypeProperty a owl:Class
;  skos:prefLabel "inverse of a datatype property"@en
.

:CreativeWork a owl:Class
;  skos:prefLabel "creative work"@en
;  :collectiveLabel "creative works"@en
;  rdfs:subClassOf skos:Concept
.

:Project a owl:Class
;  skos:prefLabel "project"@en
;  :collectiveLabel "projects"@en
;  rdfs:subClassOf :CreativeWork
;  skos:scopeNote "A creative activity with an overall focus or intent"@en
.

:Content a owl:Class
;  skos:prefLabel "content"@en
;  :collectiveLabel "content"@en
;  rdfs:subClassOf :CreativeWork
;  skos:scopeNote "A work which can be rendered in two-dimensional visual form, especially on or as a Web site"@en
.

:Site a owl:Class
;  skos:prefLabel "Web site"@en
;  :collectiveLabel "Web sites"@en
;  rdfs:subClassOf :Content
.

:Page a owl:Class
;  skos:prefLabel "Web page"@en
;  :collectiveLabel "Web pages"@en
;  rdfs:subClassOf :Content
.

:Document a owl:Class
;  skos:prefLabel "document"@en
;  :collectiveLabel "documents"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be read. Normally text based but may also be made of images (cartoon, graphic novel...)"@en
.

:Picture a owl:Class
;  skos:prefLabel "picture"@en
;  :collectiveLabel "pictures"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be viewed, whose meaning does not rely on the way it changes through time."@en
.

:Video a owl:Class
;  skos:prefLabel "video"@en
;  :collectiveLabel "videos"@en
;  rdfs:subClassOf :Content
;  skos:scopeNote "A work which is intended to be watched."@en
.

:Media a owl:Class
;  skos:prefLabel "media"@en
;  :collectiveLabel "media"@en
.

:Context a owl:Class
;  skos:prefLabel "context"@en
;  :collectiveLabel "contexts"@en
;  rdfs:subClassOf skos:Concept
.

:asPage a :Context
;  skos:prefLabel "page context"@en
.

:asEmbed a :Context
;  skos:prefLabel "embedded context"@en
.

:asLink a :Context
;  skos:prefLabel "link context"@en
.

:collectiveLabel a owl:DatatypeProperty
;  skos:prefLabel "collective label"@en
;  rdfs:domain owl:Class
;  rdfs:range xsd:string
;  rdfs:subPropertyOf rdfs:label
.

:mediaType a owl:DatatypeProperty
;  skos:prefLabel "media type"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:charset a owl:DatatypeProperty
;  skos:prefLabel "character set"@en
;  rdfs:domain :Media
;  rdfs:range xsd:string
.

:blobURL a owl:DatatypeProperty
;  skos:prefLabel "blob URL"@en
;  rdfs:domain :Media
;  rdfs:range xsd:anyURI
.

:rendition a owl:ObjectProperty
;  skos:prefLabel "rendition"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :Media
.

:renditionOf a owl:ObjectProperty
;  skos:prefLabel "rendition of"@en
;  owl:inverseOf :rendition
.

:description a owl:DatatypeProperty
;  skos:prefLabel "description"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:descriptionOf a :InverseDatatypeProperty
;  skos:prefLabel "description of"@en
;  owl:inverseOf :description
.

:markdown a owl:DatatypeProperty
;  skos:prefLabel "markdown"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:html a owl:DatatypeProperty
;  skos:prefLabel "HTML"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:string
.

:url a owl:DatatypeProperty
;  skos:prefLabel "Web URL"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:anyURI
.

:embedPath a owl:DatatypeProperty
;  skos:prefLabel "FALSE internal embed path"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:anyURI
.


:published a owl:DatatypeProperty
;  skos:prefLabel "published"@en
;  rdfs:domain :Document
;  rdfs:range xsd:dateTime
.

:updated a owl:DatatypeProperty
;  skos:prefLabel "updated"@en
;  rdfs:domain skos:Concept
;  rdfs:range xsd:dateTime
.


:features a owl:ObjectProperty
;  skos:prefLabel "features"@en
;  rdfs:domain :Site
;  rdfs:range :CreativeWork
;  rdfs:subPropertyOf skos:narrower
.

:featuresOn a owl:ObjectProperty
;  skos:prefLabel "features on"@en
;  rdfs:subPropertyOf skos:broader
;  owl:inverseOf :features
.

:includes a owl:ObjectProperty
;  skos:prefLabel "includes"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :CreativeWork
;  rdfs:subPropertyOf skos:narrower
.

:includedIn a owl:ObjectProperty
;  skos:prefLabel "included in"@en
;  rdfs:subPropertyOf skos:broader
;  owl:inverseOf :includes
.

:links a owl:ObjectProperty
;  skos:prefLabel "links to"@en
;  rdfs:domain :CreativeWork
;  rdfs:range :CreativeWork
;  rdfs:subPropertyOf skos:related
.

:linkedFrom a owl:ObjectProperty
;  skos:prefLabel "linked from"@en
;  rdfs:subPropertyOf skos:related
;  owl:inverseOf :links
.

:mentions a owl:ObjectProperty
;  skos:prefLabel "mentions"@en
;  rdfs:domain :CreativeWork
;  rdfs:range skos:Concept
;  rdfs:subPropertyOf skos:related
.

:mentionedIn a owl:ObjectProperty
;  skos:prefLabel "mentioned in"@en
;  rdfs:subPropertyOf skos:related
;  owl:inverseOf :mentions
.

:about a owl:ObjectProperty
;  skos:prefLabel "about"@en
;  rdfs:domain :CreativeWork
;  rdfs:range skos:Concept
;  rdfs:subPropertyOf skos:related
.

:hasAbout a owl:ObjectProperty
;  skos:prefLabel ""@en # !
;  rdfs:subPropertyOf skos:related
;  owl:inverseOf :about
.

:depicts a owl:ObjectProperty
;  skos:prefLabel "depicts"@en
;  rdfs:domain :Picture
;  rdfs:range skos:Concept
;  rdfs:subPropertyOf :about
.

:depictedIn a owl:ObjectProperty
;  skos:prefLabel "depicted in"@en
;  owl:inverseOf :depicts
;  rdfs:subPropertyOf :hasAbout
.

:subClass a rdf:Property
;  owl:inverseOf rdfs:subClassOf
.

:subProperty a rdf:Property
;  owl:inverseOf rdfs:subPropertyOf
.

:typeOf a rdf:Property
;  owl:inverseOf rdf:type
.

:domainOf a rdf:Property
;  owl:inverseOf rdfs:domain
.

:rangeOf a rdf:Property
;  owl:inverseOf rdfs:range
.

